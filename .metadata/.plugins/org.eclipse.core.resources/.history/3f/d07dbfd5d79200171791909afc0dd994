package com.spring.di01;

import org.springframework.stereotype.Component;

@Component(value={"mySchool"})   //组件,表示当前类被Spring容器所管理
public class School {

	private String name;

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "School [name=" + name + "]";
	}
}
